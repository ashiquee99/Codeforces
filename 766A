#include <bits/stdc++.h>
using namespace std;
#define                MX 1000006
#define                FAST_IO ios_base::sync_with_stdio(false),cin.tie(NULL)
#define                exit() return 0;

bool status[1100002];
int a[MX];
int main ()
{
    FAST_IO;
    string S1, S2;
    cin>>S1>>S2;
    if(S1!=S2)
    {
        if(S1.size()>S2.size())
        {
            cout<<S1.size();
        }
        else
        {
            cout<<S2.size();
        }
    }
    else
    {
        cout<<"-1";
    }
    return 0;
}
